<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" 
	"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
<form-beans>
	<form-bean name="clienteForm" type="org.apache.struts.action.DynaActionForm" >
		<form-property name="acao" type="java.lang.String" initial=""/>
	</form-bean>

	<form-bean name="cadastraClienteForm" type="org.apache.struts.action.DynaActionForm">
		<form-property name="acao" type="java.lang.String" initial=""/>
		<form-property name="nome" type="java.lang.String" initial=""/>
		<form-property name="telefone" type="java.lang.String" initial=""/>
		<form-property name="cpf" type="java.lang.String" initial=""/>
		<form-property name="rg" type="java.lang.String" initial=""/>
		<form-property name="email" type="java.lang.String" initial=""/>
		<form-property name="mostraResultado" type="boolean" initial="false" />
		<form-property name="detail" type="boolean" initial="false" />
		<form-property name="clientes" type="java.util.Collection" />
		<form-property name="id" type="java.lang.String" />
		<form-property name="idCliente" type="java.lang.String" initial="" />
	</form-bean>
</form-beans>
</struts-config>